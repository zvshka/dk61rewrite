{
  "name": "dk61rewrite",
  "version": "1.0.0",
  "description": "Переписанный dk61 на основе TSCord",
  "license": "MIT",
  "main": "build/main.js",
  "tscord": {
    "version": "2.4"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:compile && npm run install:plugins",
    "build:clean": "rimraf build/",
    "build:compile": "tsc --project tsconfig.json --skipLibCheck && npm run build:alias",
    "build:alias": "tsc-alias -p tsconfig.json",
    "build:start": "npm run build && npm run start",
    "install:plugins": "installoop --rootDir=./build/plugins",
    "type:check": "tsc --pretty --skipLibCheck --noEmit",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node --transpile-only src/main.ts",
    "i18n": "typesafe-i18n",
    "plugins:install": "installoop --rootDir=./src/plugins",
    "postinstall": "npm run plugins:install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@discordx/importer": "~1.3.3",
    "@discordx/pagination": "~4.4.0",
    "@discordx/utilities": "~8.0.0",
    "@napi-rs/canvas": "~0.1.90",
    "@prisma/adapter-pg": "~7.3.0",
    "@prisma/client": "~7.3.0",
    "@tsed/common": "~8.24.2",
    "@tsed/core": "~8.24.2",
    "@tsed/di": "~8.24.2",
    "@tsed/exceptions": "~8.24.2",
    "@tsed/json-mapper": "~8.24.2",
    "@tsed/platform-accept-mimes": "~8.25.0",
    "@tsed/platform-express": "~8.24.2",
    "@tsed/schema": "~8.24.2",
    "@tsed/swagger": "~8.24.2",
    "@twemoji/parser": "~17.0.1",
    "archiver": "~7.0.1",
    "axios": "~1.13.4",
    "body-parser": "~2.2.2",
    "boxen": "~8.0.1",
    "case": "~1.6.3",
    "chalk": "~5.6.2",
    "chokidar": "~5.0.0",
    "cron": "~4.4.0",
    "cron-validator": "~1.4.0",
    "cross-env": "~10.1.0",
    "dayjs": "~1.11.19",
    "discord-api-types": "~0.38.38",
    "discord-logs": "~2.2.1",
    "discord-oauth2": "~2.12.1",
    "discord.js": "~14.25.1",
    "discordx": "~11.13.3",
    "dotenv": "~17.2.3",
    "enmap": "~6.1.6",
    "envalid": "~8.1.1",
    "express": "~5.2.1",
    "fast-folder-size": "~2.4.0",
    "http-status-codes": "~2.3.0",
    "image-hash": "~7.0.1",
    "imgur": "~2.5.0",
    "installoop": "~1.1.0",
    "joi": "~18.0.2",
    "node-os-utils": "~2.0.1",
    "ora": "~9.2.0",
    "pg": "~8.18.0",
    "pidusage": "~4.0.1",
    "reflect-metadata": "~0.2.2",
    "rxeta": "~1.1.2",
    "saveqlite": "~1.1.2",
    "semver": "~7.7.3",
    "stacktrace-parser": "~0.1.11",
    "swagger-ui-express": "~5.0.1",
    "ts-toolbelt": "~9.6.0",
    "tsyringe": "~4.10.0",
    "typesafe-i18n": "~5.26.2",
    "uuid": "~13.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~2.13.0",
    "@types/archiver": "~7.0.0",
    "@types/body-parser": "~1.19.6",
    "@types/dateformat": "~5.0.3",
    "@types/express": "~5.0.6",
    "@types/node": "~25.2.0",
    "@types/node-os-utils": "~1.3.4",
    "@types/pidusage": "~2.0.5",
    "@types/semver": "~7.7.1",
    "@types/swagger-ui-express": "~4.1.8",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "ncp": "~2.0.0",
    "nodemon": "~3.1.11",
    "npm-run-all": "~4.1.5",
    "plop": "~4.0.5",
    "prisma": "~7.3.0",
    "rimraf": "~6.1.2",
    "ts-node": "~10.9.2",
    "tsc-alias": "~1.8.16",
    "tsconfig-paths": "~4.2.0",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.13.0"
  },
  "nodemonConfig": {
    "ignore": [
      "src/i18n/**/!(i18n-types.ts)",
      "src/{events,commands}/**/*.{ts,js}"
    ]
  }
}
